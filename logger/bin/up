#!/usr/bin/env bash

# Usage:
# ./bin/up pro
# ./bin/up dev

env=$1

case "$env" in
  dev)
    docker-compose -f docker-compose.yml -f docker-compose.dev.yml up
    ;;
  
  pro)
    docker-compose -f docker-compose.yml -f docker-compose.pro.yml up -d
    ;;

  *)
    echo environment not found!
esac